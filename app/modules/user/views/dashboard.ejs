<% layout(layout_directory+'/mainLayout') -%>

<section id="add-row">
    <div class="row">
        <div class="col-md-10 form-group">
            <div class="content-header">
                Top 10 Product List</div>
        </div>
        <div class="col-md-2 form-group">
            <a href="<%= generateUrl('product.viewAll.render')%>" class="btn btn-success">View All</a>
        </div>
    </div>

    <div class="row">

        <div class="col-12">
            <div class="card">
                <div class="card-body collapse show">
                    <div class="card-block">
                        <div class="hidden-scroll">

                            <table class="table table-striped table-bordered add-rows">
                                <thead>
                                    <tr>
                                        <th>Name</th>
                                        <th>SKU</th>
                                        <th>Image</th>
                                        <th>Regular Price</th>
                                        <th>Sale Price</th>
                                        <th class="subheading">
                                            <table>
                                                <tr>
                                                    <th colspan="4">Competitor 1</th>
                                                </tr>
                                                <tr>
                                                    <th>Price</th>
                                                    <th>Retail Price</th>
                                                    <th>Difference</th>
                                                    <th>Difference %</th>
                                                </tr>
                                            </table>
                                        </th>
                                        <th class="subheading">
                                            <table>
                                                <tr>
                                                    <th colspan="4">Competitor 2</th>
                                                </tr>
                                                <tr>
                                                    <th>Price</th>
                                                    <th>Retail Price</th>
                                                    <th>Difference</th>
                                                    <th>Difference %</th>
                                                </tr>
                                            </table>
                                        </th>
                                        <th class="subheading">
                                            <table>
                                                <tr>
                                                    <th colspan="4">Competitor 3</th>
                                                </tr>
                                                <tr>
                                                    <th>Price</th>
                                                    <th>Retail Price</th>
                                                    <th>Difference</th>
                                                    <th>Difference %</th>
                                                </tr>
                                            </table>
                                        </th>
                                        <th class="subheading">
                                            <table>
                                                <tr>
                                                    <th colspan="4">Competitor 4</th>
                                                </tr>
                                                <tr>
                                                    <th>Price</th>
                                                    <th>Retail Price</th>
                                                    <th>Difference</th>
                                                    <th>Difference %</th>
                                                </tr>
                                            </table>
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% if(response.data.hits.hits.length>0){ %>
                                    <% for(var i=0; i< response.data.hits.hits.length; i++) { %>
                                    <tr>
                                        <td>
                                            <div style="width:300px;">
                                                <%= response.data.hits.hits[i]._source.product_name %>
                                            </div>
                                        </td>
                                        <td>
                                            <%= response.data.hits.hits[i]._source.product_sku %>
                                        </td>
                                        <td>
                                            <% if(!_.isNull(response.data.hits.hits[i]._source.images) && response.data.hits.hits[i]._source.images.length > 0) {%>
                                            <img src="<%=response.data.hits.hits[i]._source.images[0].src%>" width="100"
                                                height="100" />
                                            <% } else { %>
                                            <img src="/images/noimage.png" width="100" height="100" />
                                            <% } %>
                                        </td>
                                        <td>
                                            <%= response.data.hits.hits[i]._source.regular_price.toFixed(2) %>
                                        </td>
                                        <td>
                                            <%= response.data.hits.hits[i]._source.sale_price.toFixed(2) %>
                                        </td>

                                        <!-- competitor 1 section-->
                                        <td class="subheading">
                                            <table>
                                                <tr>
                                                    <td>
                                                        <div style="position:relative;">
                                                            <%= response.data.hits.hits[i]._source.comp1_product_price.toFixed(2) %>
                                                            <div class="history-icon">
                                                                <a href="<%= generateUrl('product.stat', {id: response.data.hits.hits[i]._id+'_1'})%>"
                                                                    class="a-history-icon">
                                                                    <img
                                                                        src="images/calendar-with-a-clock-time-tools.png" />
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td><%= response.data.hits.hits[i]._source.comp1_product_price_retail.toFixed(2)%>
                                                    </td>
                                                    <td>
                                                        <div style="position:relative;">
                                                            <%= response.data.hits.hits[i]._source.comp1_diff_price.toFixed(2) %>
                                                            <div class="history-icon">
                                                                <% if(response.data.hits.hits[i]._source.comp1_diff_price > 0){%>
                                                                <img src="images/up_arrow.png" />
                                                                <% } else {%>
                                                                <img src="images/down_arrow.png" />
                                                                <% }%>
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <%= response.data.hits.hits[i]._source.comp1_diff_perce.toFixed(2)%>
                                                    </td>
                                                </tr>
                                            </table>
                                        </td>


                                        <!-- competitor 2 section-->
                                        <td class="subheading">
                                            <table>
                                                <tr>
                                                    <td>
                                                        <div style="position:relative;">
                                                            <%= response.data.hits.hits[i]._source.comp2_product_price.toFixed(2) %>
                                                            <div class="history-icon">
                                                                <a href="<%= generateUrl('product.stat', {id: response.data.hits.hits[i]._id+'_1'})%>"
                                                                    class="a-history-icon">
                                                                    <img
                                                                        src="images/calendar-with-a-clock-time-tools.png" />
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td><%= response.data.hits.hits[i]._source.comp2_product_price_retail.toFixed(2)%>
                                                    </td>
                                                    <td>
                                                        <div style="position:relative;">
                                                            <%= response.data.hits.hits[i]._source.comp2_diff_price.toFixed(2) %>
                                                            <div class="history-icon">
                                                                <% if(response.data.hits.hits[i]._source.comp2_diff_price > 0){%>
                                                                <img src="images/up_arrow.png" />
                                                                <% } else {%>
                                                                <img src="images/down_arrow.png" />
                                                                <% }%>
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <%= response.data.hits.hits[i]._source.comp2_diff_perce.toFixed(2)%>
                                                    </td>
                                                </tr>
                                            </table>
                                        </td>

                                        <!-- competitor 3 section-->
                                        <td class="subheading">
                                            <table>
                                                <tr>
                                                    <td>
                                                        <div style="position:relative;">
                                                            <%= response.data.hits.hits[i]._source.comp3_product_price.toFixed(2) %>
                                                            <div class="history-icon">
                                                                <a href="<%= generateUrl('product.stat', {id: response.data.hits.hits[i]._id+'_1'})%>"
                                                                    class="a-history-icon">
                                                                    <img
                                                                        src="images/calendar-with-a-clock-time-tools.png" />
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td><%= response.data.hits.hits[i]._source.comp3_product_price_retail.toFixed(2)%>
                                                    </td>
                                                    <td>
                                                        <div style="position:relative;">
                                                            <%= response.data.hits.hits[i]._source.comp3_diff_price.toFixed(2) %>
                                                            <div class="history-icon">
                                                                <% if(response.data.hits.hits[i]._source.comp3_diff_price > 0){%>
                                                                <img src="images/up_arrow.png" />
                                                                <% } else {%>
                                                                <img src="images/down_arrow.png" />
                                                                <% }%>
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <%= response.data.hits.hits[i]._source.comp3_diff_perce.toFixed(2) %>
                                                    </td>
                                                </tr>
                                            </table>
                                        </td>

                                        <!-- competitor 4 section-->
                                        <td class="subheading">
                                            <table>
                                                <tr>
                                                    <td>
                                                        <div style="position:relative;">
                                                            <%= response.data.hits.hits[i]._source.comp4_product_price.toFixed(2) %>
                                                            <div class="history-icon">
                                                                <a href="<%= generateUrl('product.stat', {id: response.data.hits.hits[i]._id+'_1'})%>"
                                                                    class="a-history-icon">
                                                                    <img
                                                                        src="images/calendar-with-a-clock-time-tools.png" />
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td><%= response.data.hits.hits[i]._source.comp4_product_price_retail.toFixed(2)%>
                                                    </td>
                                                    <td>
                                                        <div style="position:relative;">
                                                            <%= response.data.hits.hits[i]._source.comp4_diff_price.toFixed(2) %>
                                                            <div class="history-icon">
                                                                <% if(response.data.hits.hits[i]._source.comp4_diff_price > 0){%>
                                                                <img src="images/up_arrow.png" />
                                                                <% } else {%>
                                                                <img src="images/down_arrow.png" />
                                                                <% }%>
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <%= response.data.hits.hits[i]._source.comp4_diff_perce.toFixed(2) %>
                                                    </td>
                                                </tr>
                                            </table>
                                        </td>
                                    </tr>
                                    <% } %>
                                    <% }else{ %>
                                    <tr>
                                        <td colspan="9">
                                            <center>No Record Found</center>
                                        </td>
                                    </tr>
                                    <% } %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</section>